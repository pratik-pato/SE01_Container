<?xml version="1.0" encoding="ISO-8859-15"?>
<wallTemplate>
  <objectID> unique object id </objectID>
  <objectType> type id </objectType>
  <userDefinedObjectID> string </userDefinedObjectID>
  <userLabel> string </userLabel>
  <userComment> string </userComment>

  <wallTemplateID> string </wallTemplateID>
  <rectDims>
    <height> int </height>
    <width> int </width>
  </rectDims>
  <color>
    (int, int, int)
  </color>
   <!--
     if there are no doors on the wall, then numberOfDoors will contain 0
     and
     the tag doorlist will be empty
      -->
  <numberOfDoors> int </numberOfDoors>
  <doorlist>
    <doorPlacement>
      <doorTemplateID> string </doorTemplateID>
      <doorOrientation> (char, char,char, char) </doorOrientation>
      <doorDisplacement> (int, int) </doorDisplacement>
    </doorPlacement>
  </doorlist>
</wallTemplate>


